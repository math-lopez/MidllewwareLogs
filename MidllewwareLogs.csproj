<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>MATHJES</Authors>
    <Company>MATHJES TECH</Company>
    <PackageId>MidllewwareLogs.LoggingMiddleware</PackageId>
    <Title>Logging Middleware</Title> 
    <Description>Um middleware para logar requisições e respostas com um serviceId fixo.</Description>
    <PackageTags>middleware;logging;aspnetcore</PackageTags>
    <RepositoryUrl>https://github.com/math-lopez/SeuRepositorio</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Http.Abstractions" Version="2.2.0" />
  </ItemGroup>

</Project>
